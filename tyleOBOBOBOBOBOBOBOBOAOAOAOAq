           Configuration for Waybar, see [1mhttps://github.com/Alexays/Waybar/wiki/Configuration[22m[1] for
           supported values.

           [4mType:[24m (list of (JSON value)) or attribute set of (JSON value)

           [4mDefault:[24m [ ]

           [4mExample:[0m

               {
                 mainBar = {
                   layer = "top";
                   position = "top";
                   height = 30;
                   output = [
                     "eDP-1"
                     "HDMI-A-1"
                   ];
                   modules-left = [ "sway/workspaces" "sway/mode" "wlr/taskbar" ];
                   modules-center = [ "sway/window" "custom/hello-from-waybar" ];
                   modules-right = [ "mpd" "custom/mymodule#with-css-id" "temperature" ];

                   "sway/workspaces" = {
                     disable-scroll = true;
                     all-outputs = true;
                   };
                   "custom/hello-from-waybar" = {
                     format = "hello {}";
                     max-length = 40;
                     interval = "once";
                     exec = pkgs.writeShellScript "hello-from-waybar" ''
                       echo "from within waybar"
                     '';
                   };
                 };
               }

